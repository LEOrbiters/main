{
  "name": "leo-orbiters-flutter",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "python server.py",
    "client": "cd src1 && flutter run -d chrome --web-port 3001",
    "build": "npm run build:flutter",
    "build:flutter": "cd src1 && flutter build web",
    "clean": "cd src1 && flutter clean",
    "require": "python3 -m pip install -r requirements.txt || python -m pip install -r requirements.txt"

  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "concurrently": "^9.2.1"
  }
}
